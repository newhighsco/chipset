{
  "name": "@newhighsco/chipset",
  "description": "",
  "version": "1.5.3",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Chipset"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://chipset.newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "start-storybook --port 9001 --static-dir .storybook/static --docs --ci --quiet",
    "prebuild": "rm -rf build",
    "build": "build-storybook --output-dir build --static-dir .storybook/static --docs --quiet",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@friendsofreactjs/react-css-themr": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@newhighsco/browserslist-config": "1.0.0",
    "@newhighsco/commitlint-config": "1.0.0",
    "@newhighsco/editor-config": "1.0.2",
    "@newhighsco/eslint-config": "2.0.3",
    "@newhighsco/prettier-config": "1.1.1",
    "@newhighsco/release-config": "1.0.19",
    "@newhighsco/stylelint-config": "2.0.4",
    "@storybook/addon-docs": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/react": "5.3.17",
    "babel-loader": "8.1.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "sass-loader": "8.0.2",
    "semantic-release": "17.0.4",
    "stylelint": "13.2.1"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "browserslist": {
    "extends": [
      "@newhighsco/browserslist-config"
    ]
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
