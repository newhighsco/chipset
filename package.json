{
  "name": "@newhighsco/chipset",
  "description": "Shareable, theme-able component library by New High Score",
  "version": "6.13.6",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "theme": "dark",
    "title": "Chipset",
    "logo": "https://chipset.newhighsco.re/logo.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://chipset.newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "storybook": {
    "url": "https://main--6004028a0df11e0021f58df8.chromatic.com"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf {build,coverage,dist}",
    "start": "start-storybook --docs --no-manager-cache",
    "build": "npm-run-all --parallel build:*",
    "prebuild:package": "rm -rf dist",
    "build:package": "rollup -c",
    "prebuild:storybook": "rm -rf build",
    "build:storybook": "build-storybook --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@dr.pogodin/react-themes": "1.4.1",
    "classnames": "2.3.1",
    "react-device-detect": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@commitlint/cli": "16.2.1",
    "@newhighsco/browserslist-config": "1.0.3",
    "@newhighsco/commitlint-config": "1.0.23",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "3.1.13",
    "@newhighsco/postcss-config": "3.4.66",
    "@newhighsco/prettier-config": "2.0.16",
    "@newhighsco/release-config": "1.1.6",
    "@newhighsco/storybook-preset": "4.4.24",
    "@newhighsco/stylelint-config": "3.0.17",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.0",
    "@rollup/plugin-url": "6.1.0",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@svgr/rollup": "6.2.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@types/css-modules": "1.0.2",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.2.3",
    "chromatic": "6.5.1",
    "eslint": "8.10.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.8",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.67.1",
    "rollup-plugin-dts": "4.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "sass-true": "6.1.0",
    "semantic-release": "19.0.2",
    "stylelint": "14.5.3",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "webpack": "5.70.0"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
