{
  "name": "@newhighsco/chipset",
  "description": "Shareable, theme-able component library by New High Score",
  "version": "6.6.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Chipset",
    "logo": "logo.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://chipset.newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "src/index.js",
  "files": [
    "src",
    "styles"
  ],
  "scripts": {
    "start": "start-storybook --static-dir .storybook/static --docs",
    "prebuild": "rm -rf build",
    "build": "build-storybook --output-dir build --static-dir .storybook/static --docs --quiet",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --maxWorkers=2 --coverage",
    "tdd": "npm run test:js -- --watch",
    "snapshot": "npm-run-all --parallel snapshot:*",
    "snapshot:chromatic": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@dr.pogodin/react-themes": "1.3.2",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@newhighsco/storybook-preset": "3.3.26",
    "classnames": "2.3.1",
    "react-device-detect": "1.17.0",
    "react-ga": "3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@commitlint/cli": "12.1.4",
    "@newhighsco/browserslist-config": "1.0.2",
    "@newhighsco/commitlint-config": "1.0.12",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.90",
    "@newhighsco/postcss-config": "3.0.101",
    "@newhighsco/prettier-config": "2.0.9",
    "@newhighsco/release-config": "1.0.60",
    "@newhighsco/stylelint-config": "2.0.38",
    "@storybook/addon-docs": "6.2.9",
    "@storybook/react": "6.2.9",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "chromatic": "5.8.3",
    "enzyme": "3.11.0",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass-true": "6.0.1",
    "semantic-release": "17.4.3",
    "stylelint": "13.13.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
