{
  "name": "@newhighsco/chipset",
  "description": "",
  "version": "1.3.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://github.com/newhighsco/chipset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "src/index.js",
  "style": "styles/_chipset.scss",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@newhighsco/browserslist-config": "1.0.0",
    "@newhighsco/commitlint-config": "1.0.0",
    "@newhighsco/editor-config": "1.0.0",
    "@newhighsco/eslint-config": "2.0.1",
    "@newhighsco/prettier-config": "1.0.3",
    "@newhighsco/release-config": "1.0.16",
    "@newhighsco/stylelint-config": "2.0.1",
    "classnames": "2.2.6",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "semantic-release": "17.0.4",
    "stylelint": "13.2.1"
  },
  "peerDependencies": {
    "classnames": "2.2.6",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "browserslist": {
    "extends": [
      "@newhighsco/browserslist-config"
    ]
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
