{
  "name": "@newhighsco/chipset",
  "description": "Shareable, theme-able component library by New High Score",
  "version": "6.14.14",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Chipset",
    "logo": "https://chipset.newhighsco.re/logo.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://chipset.newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "src/index.js",
  "files": [
    "src",
    "styles"
  ],
  "storybook": {
    "url": "https://main--6004028a0df11e0021f58df8.chromatic.com"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "start-storybook --port 49928 --docs --no-manager-cache",
    "prebuild": "rm -rf build",
    "build": "build-storybook --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --maxWorkers=2 --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "lint:css": "stylelint --cache --ignore-path .gitignore '**/*.?(s)css'",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@dr.pogodin/react-themes": "1.4.7",
    "classnames": "2.3.2",
    "react-device-detect": "2.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.4.4",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.15",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.16",
    "@newhighsco/postcss-config": "3.5.117",
    "@newhighsco/prettier-config": "2.1.8",
    "@newhighsco/release-config": "1.3.15",
    "@newhighsco/storybook-preset": "4.5.68",
    "@newhighsco/stylelint-config": "3.1.40",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "babel-loader": "9.1.2",
    "chromatic": "6.17.1",
    "eslint": "8.34.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "is-ci": "3.0.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-environment-node-single-context": "29.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass-true": "7.0.0",
    "semantic-release": "20.1.0",
    "stylelint": "15.2.0",
    "webpack": "5.75.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
