{
  "name": "@newhighsco/chipset",
  "description": "Shareable, theme-able component library by New High Score",
  "version": "4.0.3",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Chipset",
    "logo": "logo.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://chipset.newhighsco.re/",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "start-storybook --port 9001 --static-dir .storybook/static --docs --ci --quiet",
    "prebuild": "rm -rf build",
    "build": "build-storybook --output-dir build --static-dir .storybook/static --docs --quiet",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@friendsofreactjs/react-css-themr": "4.1.0",
    "classnames": "2.2.6",
    "react-ga": "3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@commitlint/cli": "9.0.1",
    "@newhighsco/browserslist-config": "1.0.2",
    "@newhighsco/commitlint-config": "1.0.2",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.10",
    "@newhighsco/prettier-config": "2.0.3",
    "@newhighsco/release-config": "1.0.26",
    "@newhighsco/storybook-preset": "2.3.9",
    "@newhighsco/stylelint-config": "2.0.18",
    "@storybook/react": "5.3.19",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.4.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass-true": "5.0.0",
    "semantic-release": "17.1.1",
    "stylelint": "13.6.1"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
