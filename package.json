{
  "name": "@newhighsco/chipset",
  "description": "Shareable, theme-able component library by New High Score",
  "version": "6.27.3",
  "author": "New High Score",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/chipset.git"
  },
  "homepage": "https://github.com/newhighsco/chipset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/chipset/issues"
  },
  "packageManager": "yarn@4.12.0",
  "main": "src/index.js",
  "files": [
    "src",
    "styles"
  ],
  "storybook": {
    "url": "https://main--6004028a0df11e0021f58df8.chromatic.com"
  },
  "scripts": {
    "prepare": "husky",
    "start": "storybook dev --port 49928 --docs",
    "build": "storybook build --output-dir build --docs --quiet --webpack-stats-json",
    "pretest": "yarn lint",
    "test": "jest --runInBand",
    "test:list": "jest --listTests",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded --only-changed",
    "analyze": "omlet analyze",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint --cache --ignore-path .gitignore '**/*.?(s)css'",
    "format": "concurrently 'yarn:format:*'",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@dr.pogodin/react-themes": "1.10.3",
    "classnames": "2.5.1",
    "react-device-detect": "2.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "@commitlint/cli": "20.4.1",
    "@newhighsco/browserslist-config": "2.0.0",
    "@newhighsco/commitlint-config": "1.1.48",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "7.0.5",
    "@newhighsco/postcss-config": "4.0.19",
    "@newhighsco/prettier-config": "2.3.3",
    "@newhighsco/release-config": "1.4.24",
    "@newhighsco/storybook-preset": "10.2.5",
    "@newhighsco/stylelint-config": "4.0.25",
    "@newhighsco/svgr-config": "1.0.0",
    "@omlet/cli": "2.0.0",
    "@storybook/react-webpack5": "10.2.8",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "babel-loader": "10.0.0",
    "chromatic": "15.1.0",
    "concurrently": "9.2.1",
    "eslint": "10.0.0",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-junit": "16.0.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sass-true": "10.1.0",
    "semantic-release": "25.0.3",
    "storybook": "10.2.8",
    "stylelint": "17.2.0",
    "webpack": "5.105.1"
  },
  "peerDependencies": {
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "omlet": {
    "exports": {
      ".": "src/index.js"
    },
    "ignore": [
      "example/**"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ],
    "overrides": [
      {
        "files": [
          "**/*.spec.*"
        ],
        "rules": {
          "number-max-precision": null
        }
      }
    ],
    "rules": {
      "value-keyword-case": [
        "lower",
        {
          "camelCaseSvgKeywords": true,
          "ignoreKeywords": [
            "/(mobile|tablet)Landscape/",
            "/desktop(Medium|Large)/"
          ]
        }
      ]
    }
  }
}
